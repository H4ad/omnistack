NODE_ENV=development

# Database Configurations
DB_TYPE=postgres
DB_PORT=5432
DB_HOST=localhost
DB_USER=root
DB_PASSWORD=1234
DB_DATABASE=dev
DB_SYNCHRONIZE=false
DB_MIGRATIONS_RUN=true
DB_TIMEOUT=60000

# API Configurations
API_DEFAULT_STRATEGY=jwt
API_BASE_PATH=
# Quando alterar a porta da API, altere tamb√©m o upstream no arquivo nginx/sites-available/api_insecure.conf
PORT=3000

# JWT Configurations
JWT_SECRET_KEY=CHANGE_THIS_SECRET
JWT_EXPIRES_IN=7d

# Swagger Configurations
SWAGGER_TITLE=Base API
SWAGGER_DESCRIPTION=Base
SWAGGER_VERSION=1.0
SWAGGER_TAG=Base
SWAGGER_ENABLED=true

# Sentry Configuration
SENTRY_DNS=
